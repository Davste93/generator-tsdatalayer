{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": false,
        "moduleResolution": "node"
    },
    "filesGlob": [
        "app.ts",
        "models/**.ts",
        "data/**.ts",
        "typings/main.d.ts"
    ],
    "files": [
        "app.ts",
        "models/apiAccount.ts",
        "models/apiAccountEntry.ts",
        "models/apiAccountPermission.ts",
        "models/apiAccountRule.ts",
        "models/apiFee.ts",
        "data/apiAccountDataRepositoryImpl.ts",
        "data/apiAccountEntryDataRepositoryImpl.ts",
        "data/apiAccountPermissionDataRepositoryImpl.ts",
        "data/apiAccountRuleDataRepositoryImpl.ts",
        "data/apiFeeDataRepositoryImpl.ts",
        "typings/main.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
