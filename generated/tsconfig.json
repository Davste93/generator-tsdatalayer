{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        "sourceMap": false,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "outDir": "build"
    },
    "filesGlob": [
        "app.ts",
        "models/**.ts",
        "ApiResponseParsers/**.ts",
        "Auth/**.ts",
        "data/**.ts",
        "services/**.ts",
        "spec/**/*.ts",
        "typings/index.d.ts",
        "customTypings/typings.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    },
    "files": [
        "app.ts",
        "models/apiAccount.ts",
        "models/apiAccountEntry.ts",
        "models/apiAccountPermission.ts",
        "models/apiAccountRule.ts",
        "models/apiFee.ts",
        "ApiResponseParsers/HateoasResponseParser.ts",
        "Auth/BasicAuth.ts",
        "Auth/KeycloakApiDecorator.ts",
        "data/apiAccountDataRepository.ts",
        "data/apiAccountDataRepositoryImpl.ts",
        "data/apiAccountEntryDataRepository.ts",
        "data/apiAccountEntryDataRepositoryImpl.ts",
        "data/apiAccountPermissionDataRepository.ts",
        "data/apiAccountPermissionDataRepositoryImpl.ts",
        "data/apiAccountRuleDataRepository.ts",
        "data/apiAccountRuleDataRepositoryImpl.ts",
        "data/apiFeeDataRepository.ts",
        "data/apiFeeDataRepositoryImpl.ts",
        "services/apiAccountEntryService.ts",
        "services/apiAccountPermissionService.ts",
        "services/apiAccountRuleService.ts",
        "services/apiAccountService.ts",
        "services/apiFeeService.ts",
        "services/serviceManager.ts",
        "spec/services/apiAccount.e2e.spec.ts",
        "spec/services/apiAccount.spec.ts",
        "spec/services/apiAccountEntry.e2e.spec.ts",
        "spec/services/apiAccountEntry.spec.ts",
        "spec/services/apiAccountPermission.e2e.spec.ts",
        "spec/services/apiAccountPermission.spec.ts",
        "spec/services/apiAccountRule.e2e.spec.ts",
        "spec/services/apiAccountRule.spec.ts",
        "spec/services/apiFee.e2e.spec.ts",
        "spec/services/apiFee.spec.ts",
        "typings/index.d.ts",
        "customTypings/typings.d.ts"
    ]
}
